  inherited qry_Padrao: TFDQuery
    UpdateOptions.AssignedValues = [uvGeneratorName]
    UpdateOptions.GeneratorName = 'GEN_FORNECEDOR_ID'
    UpdateOptions.AutoIncFields = 'ID'
    SQL.Strings = (
      'SELECT'
      '    FORNECEDOR.ID,'
      '    FORNECEDOR.NOME,'
      '    FORNECEDOR.ENDERECO,'
      '    FORNECEDOR.NUMERO,'
      '    FORNECEDOR.BAIRRO,'
      '    FORNECEDOR.CIDADE,'
      '    FORNECEDOR.UF,'
      '    FORNECEDOR.CEP,'
      '    FORNECEDOR.TELEFONE,'
      '    FORNECEDOR.CNPJ,'
      '    FORNECEDOR.CADASTRO,'
      '    FORNECEDOR.TIPO '
      'from FORNECEDOR')
    object qry_PadraoID: TFDAutoIncField
      FieldName = 'ID'
      Origin = 'ID'
      ProviderFlags = [pfInWhere, pfInKey]
      ReadOnly = True
    end
    object qry_PadraoNOME: TStringField
      FieldName = 'NOME'
      Origin = 'NOME'
      Required = True
      Size = 100
